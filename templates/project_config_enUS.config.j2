appName = {{ item.PROJECT_NAME }}

[{{ item.PROJECT_NAME }}]
languageName=enUS
appPath_enUS={{ item.PROJECT_SRC }}
domainName=heytate.com
domainPackageName={{ item.PROJECT_JAVA_PACKAGE }}
suffixSrcMainJava=/src/main/java
suffixSrcGenJava=/src/gen/java
relativePathsToWatch=src/main/java/{{ item.PROJECT_JAVA_PACKAGE | replace('.', '/') }}
jdbcHost="{{ POSTGRES_HOST_NAME }}"
jdbcPort=5432
jdbcDatabase="{{ PROJECT_DB_NAME_ENUS }}"
jdbcUsername={{ PROJECT_DB_USER_ENUS }}
jdbcPassword="{{ PROJECT_DB_PASSWORD_ENUS }}"
siteHostName={{ PROJECT_SITE_HOST_NAME_ENUS }}
siteBaseUrl={{ PROJECT_SITE_BASE_URL_ENUS }}
authRealm={{ AUTH_REALM }}
authResource={{ AUTH_RESOURCE }}
authSecret={{ AUTH_SECRET }}
authUrl={{ AUTH_URL }}
authSslRequired=all
sitePort={{ PROJECT_SITE_PORT_ENUS }}
solrUrl=http://{{ SOLR_HOST_NAME }}:{{ SOLR_PORT }}/solr/{{ PROJECT_SOLR_COLLECTION_ENUS }}
solrUrlComputate=http://{{ SOLR_HOST_NAME }}:{{ SOLR_PORT }}/solr/computate
apiContactEmail={{ EMAIL_FROM }}
siteZone={{ PROJECT_SITE_ZONE_ENUS }}
staticBaseUrl={{ PROJECT_STATIC_BASE_URL }}
staticPath={{ item.PROJECT_SRC }}-static
emailHost=
emailPort=
emailSsl=
emailAuth=
emailUsername=
emailPassword=
emailFrom=
emailAdmin=
sqlTables={{ PROJECT_CONFIG.sqlTables | default(true) }}
// Set the userKey of the user creating an object in the database. 
enableUserKey={{ PROJECT_CONFIG.enableUserKey | default(true) | lower }}
// Set the sessionId of the user session creating an object in the database. 
enableSessionId={{ PROJECT_CONFIG.enableSessionId | default(true) | lower }}
// Enable an object in the database to be deleted. 
enableDeleted={{ PROJECT_CONFIG.enableDeleted | default(true) | lower }}
// Enable an object in the database to be archived. 
enableArchived={{ PROJECT_CONFIG.enableArchived | default(true) | lower }}
